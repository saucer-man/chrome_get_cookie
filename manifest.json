{
  "name": "getCookie",
  "manifest_version": 2,
  "version": "1.0",
  "description": "getCookie 扩展程序",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["getcookie.js"],
    "persistent": false
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "cookies"
  ]
}
